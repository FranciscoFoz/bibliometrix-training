@article{ WOS:000340152900003,
Author = {Irfan, Rabia and Khan, Sharifullah and Qamar, Ali Mustafa and
   Bloodsworth, Peter Charles},
Title = {Refining Kea plus plus automatic keyphrase assignment},
Journal = {JOURNAL OF INFORMATION SCIENCE},
Year = {2014},
Volume = {40},
Number = {4},
Pages = {446-459},
Month = {AUG},
Abstract = {Keyphrases facilitate finding the right information in digital sources.
   Keyphrase assignment is the alignment of documents or text with
   keyphrases of any standard taxonomy/classification system. Kea++ is an
   automatic keyphrase assignment tool using a machine learning-based
   technique. However, it does not effectively exploit the hierarchical
   relations that exist in its input taxonomy and returns noise in its
   results. The refinement methodology was designed as a top layer of Kea++
   in order to fine tune its results. It was an initial step and focused on
   a single Computing domain. It was neither validated on multiple domains
   nor evaluated to determine whether the improvement in the results is
   significant or not. The aim of this task was to solidify the refinement
   methodology. The main contributions of this work are (a) to extend the
   methodology for multiple domains and (b) to statistically verify that
   the improvement in the Kea++ results is significant.},
