@inproceedings{ WOS:000166277000014,
Author = {Lewis, B},
Editor = {Aparac, T and Saracevic, T and Ingwersen, P and Vakkari, P},
Title = {Automating electronic document organisation},
Booktitle = {DIGITAL LIBRARIES: INTERDISCIPLINARY CONCEPTS, CHALLENGES AND
   OPPORTUNITIES, COLIS3 PROCEEDINGS},
Year = {1999},
Pages = {165-177},
Note = {3rd International Conference on Conceptions of Library and Information
   Science, DUBROVNIK, CROATIA, MAY 23-26, 1999},
Abstract = {The organisation of an electronic document collection can utilise the
   principles which have been developed for non-electronic document
   collections. The application of those principles in a different
   environment is not a simple one, but should try to take advantage of the
   capabilities that the new environment has. The capabilities of the
   information technology environment which we are investigating are those
   of artificial intelligence, machine learning and information retrieval.
   We are investigating combining these capabilities in a library context.
   We describe an experimental automatic document classification system,
   outlining its design, a series of tests and the results obtained so far.
   The results of these tests suggest that the design approach of the
   system is viable. This is suggestive for further work in both machine
   learning and automated document classification.},
