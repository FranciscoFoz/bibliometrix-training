@article{ WOS:000501082000006,
Author = {Barham, Reham Shawqi and Sharieh, Ahmad and Sleit, Azzam},
Title = {A meta-heuristic framework based on clustering and preprocessed datasets
   for solving the link prediction problem},
Journal = {JOURNAL OF INFORMATION SCIENCE},
Year = {2019},
Volume = {45},
Number = {6},
Pages = {794-817},
Month = {DEC},
Abstract = {This study presents a solution to a problem commonly known as link
   prediction problem. Link prediction problem interests in predicting the
   possibility of appearing a connection between two nodes of a network,
   while there is no connection between these nodes in the present state of
   the network. Finding a solution to link prediction problem attracts
   variety of computer science fields such as data mining and machine
   learning. This attraction is due to its importance for many applications
   such as social networks, bioinformatics and co-authorship networks.
   Towards solving this problem, Evolutionary Link Prediction (EVO-LP)
   framework is proposed, presented, analysed and tested. EVO-LP is a
   framework that includes dataset preprocessing approach and a
   meta-heuristic algorithm based on clustering for prediction. EVO-LP is
   divided into preprocessing stage and link prediction stage. Feature
   extraction, data under-sampling and feature selection are utilised in
   the preprocessing stage, while in the prediction stage, a meta-heuristic
   algorithm based on clustering is used as an optimiser. Experimental
   results on a number of real networks show that EVO-LP improves the
   prediction quality with low time complexity.},
Type = {Article},
Affiliation = {Sleit, A (Corresponding Author), Univ Jordan, POB 13898, Amman 11942, Jordan.
   Barham, Reham Shawqi; Sharieh, Ahmad; Sleit, Azzam, Univ Jordan, KASIT, Comp Sci Dept, Amman, Jordan.
   Sleit, Azzam, Qatar Univ, KINDI Ctr Comp Res, Doha, Qatar.},
DOI = {10.1177/0165551518816296},
ISSN = {0165-5515},
EISSN = {1741-6485},
Keywords = {Data clustering; data under-sampling; feature extraction; feature
   selection; link prediction; meta-heuristic algorithms; similarity
   measures},
Keywords-Plus = {SELECTION},
Research-Areas = {Computer Science; Information Science \& Library Science},
Web-of-Science-Categories  = {Computer Science, Information Systems; Information Science \& Library
