@article{ WOS:000773181300001,
Author = {Zhang, Lin and Sun, Beibei and Shu, Fei and Huang, Ying},
Title = {Comparing paper level classifications across different methods and
   systems: an investigation of Nature publications},
Journal = {SCIENTOMETRICS},
