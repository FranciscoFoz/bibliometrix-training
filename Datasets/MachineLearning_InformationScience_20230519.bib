@article{ WOS:000669503200010,
Author = {de Souza, Marcos and Souza, Renato Rocha},
Title = {Mapping of scientific knowledge: modeling of the graduate program in
   Information Science of the Federal University of Minas Gerais},
Journal = {EM QUESTAO},
