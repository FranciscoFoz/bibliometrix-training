@article{ WOS:000305233900011,
Author = {Cruz Diaz, Noa P. and Mana Lopez, Manuel J. and Mata Vazquez, Jacinto
   and Pachon Alvarez, Victoria},
Title = {A Machine-Learning Approach to Negation and Speculation Detection in
   Clinical Texts},
