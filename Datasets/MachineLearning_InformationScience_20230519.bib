@article{ WOS:000368340100002,
Author = {Gorrell, Genevieve and Bontcheva, Kalina},
Title = {Classifying Twitter favorites: Like, bookmark, or Thanks?},
Journal = {JOURNAL OF THE ASSOCIATION FOR INFORMATION SCIENCE AND TECHNOLOGY},
Year = {2016},
Volume = {67},
Number = {1},
Pages = {17-25},
Month = {JAN},
Abstract = {Since its foundation in 2006, Twitter has enjoyed a meteoric rise in
   popularity, currently boasting over 500 million users. Its short text
   nature means that the service is open to a variety of different usage
   patterns, which have evolved rapidly in terms of user base and
   utilization. Prior work has categorized Twitter users, as well as
   studied the use of lists and re-tweets and how these can be used to
   infer user profiles and interests. The focus of this article is on
   studying why and how Twitter users mark tweets as favoritesa
   functionality with currently poorly understood usage, but strong
   relevance for personalization and information access applications.
   Firstly, manual analysis and classification are carried out on a
   randomly chosen set of favorited tweets, which reveal different
   approaches to using this functionality (i.e., bookmarks, thanks, like,
   conversational, and self-promotion). Secondly, an automatic favorites
   classification approach is proposed, based on the categories established
   in the previous step. Our machine learning experiments demonstrate a
   high degree of success in matching human judgments in classifying
   favorites according to usage type. In conclusion, we discuss the
   purposes to which these data could be put, in the context of identifying
   users' patterns of interests.},
Type = {Article},
Affiliation = {Gorrell, G (Corresponding Author), Univ Sheffield, Dept Comp Sci, 211 Portobello, Sheffield S1 4DP, S Yorkshire, England.
